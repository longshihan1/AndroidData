{
  "title": "Java/Android 笔试、面试 知识整理",
  "author": "Longshihan",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "disqus",
    "ga",
    "-highlight",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
    "mathjax@https://github.com/OrgVue/gitbook-plugin-mathjax.git#speech-fix"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord" : ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "disqus": {
      "shortName": "gitbook-template"
    },
    "ga": {
      "token": "UA-131910384-3"
    },
    "page-footer-ex": {
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "mathjax":{
      "forceSVG": false
    }
  }
}
